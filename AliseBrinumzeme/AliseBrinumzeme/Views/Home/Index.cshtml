                    </div>
                    <div class="lear-container">
                        <img src="~/Content/images/Rabbit_Ears_002_LEar_001.png" alt="Alternate Text" />
                    </div>
                    <div class="rear-container">
                        <img src="~/Content/images/Rabbit_Ears_002_REar_001.png" alt="Alternate Text" />
                    </div>
                </div>

                <!-- #endregion -->

            </div>
        </div>

