        </div>

        <div id="doors-wrap">
            <div class="doors-container">
                <!-- Door Image -->
                <img src="~/Content/images/Doors_001.png" alt="Doors" class="doors-image" width="324" height="724" />

                <!-- #region Menu Hitboxes -->
                <!--
                    Section 1:  Pasākumu Organizēšana
                    Section 2:  Grims
                    Section 3:  Fotosesijas
                    Section 4:  Dekorācijas
                    Section 5:  Karnevālu tērpu un aksesuāru noma
                    Section 6:  Par Darbnīcu
                -->

                <div class="menu-hitbox section-1" data-menutextindex="1"></div>
                <div class="menu-hitbox section-2" data-menutextindex="2"></div>
                <div class="menu-hitbox section-3" data-menutextindex="3"></div>
                <div class="menu-hitbox section-4" data-menutextindex="4"></div>
                <div class="menu-hitbox section-5" data-menutextindex="5"></div>
                <div class="menu-hitbox section-6" data-menutextindex="6"></div>

                <!-- #endregion -->
                <!-- #region Bubble Bird -->

                <div class="bubble-bird-wrap">
                    <div class="bubble-bird-container">
                        <img src="~/Content/images/BubbleBird_BodyNoLegs.png" alt="" class="bubble-bird-body" width="150" />
                        <img src="~/Content/images/BubbleBird_TBeak.png" alt="" class="bubble-bird-top-beak" width="15" />
                        <img src="~/Content/images/BubbleBird_BBeak.png" alt="" class="bubble-bird-bottom-beak" width="15" />
                        <img src="~/Content/images/BubbleBird_Legs.png" alt="" class="bubble-bird-legs" width="40" />
                    </div>
                </div>

                <!-- #endregion -->
                <!-- #region Bubble Text -->

                <div class="bubble-text-wrap" data-html2canvas-ignore="true">
                    <div class="bubble-text-container">
                        <img width="235" height="123" src="/Content/images/Bubble_PO_V2_001.png" alt="Pasākumu Organizēšana" class="bubble-text-item" data-bubbletextindex="1" />
                        <img width="235" height="123" src="/Content/images/Bubble_G_V2_001.png" alt="Grims" class="bubble-text-item" data-bubbletextindex="2" />
                        <img width="235" height="123" src="/Content/images/Bubble_F_V2_001.png" alt="Fotosesijas" class="bubble-text-item" data-bubbletextindex="3" />
                        <img width="235" height="123" src="/Content/images/Bubble_D_V2_001.png" alt="Dekorācijas" class="bubble-text-item" data-bubbletextindex="4" />
                        <img width="235" height="123" src="/Content/images/Bubble_KTUAN_V2_001.png" alt="Karnevālu tērpu un aksesuāru noma" class="bubble-text-item" data-bubbletextindex="5" />
                        <img width="235" height="123" src="/Content/images/Bubble_PD_V2_001.png" alt="Par Darbnīcu" class="bubble-text-item" data-bubbletextindex="6" />
                    </div>
                </div>

